{
  "name": "apimtrigger",
  "displayName": "apimTrigger",
  "description": "",
  "version": "1.3.0",
  "publisher": "Demon-APIM",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "json",
        "extensions": [
          ".json"
        ],
        "aliases": [
          "JSON"
        ]
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.apimTrigger",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.apimTrigger",
        "title": "Show JSON Value Message"
      }
    ],
    "keybindings": [
      {
        "command": "extension.apimTrigger",
        "key": "ctrl+alt+f",
        "mac": "ctrl+alt+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
